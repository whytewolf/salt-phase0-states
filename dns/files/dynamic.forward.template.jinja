$TTL 604800
@       IN      SOA     ns.{{domain}}. {{domain}}. (
                          {{"now"|strftime('%Y%m%d')}}01         ; Serial
                         604800         ; Refresh
                          86400         ; Retry
                        2419200         ; Expire
                         604800 )       ; Negative Cache TTL
;
@       IN      NS      ns.{{domain}}.
ns      IN      A       {{nameserver}}
